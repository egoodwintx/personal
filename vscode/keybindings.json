[
// egoodwin keybindings
// author : Ed Goodwin
// date : 03.24.2023
//

// Global keybindings
    // open up terminal and focus in terminal window
    {
        "key": "Ctrl+Shift+`", "command": "workbench.action.terminal.focus"
    },
    // toggle focus from terminal back to file editor
    {
        "key":     "ctrl+`",
        "command": "workbench.action.focusActiveEditorGroup",
        "when":    "terminalFocus"
    }

]